   "outputs": [],
   "source": [
    "# Create filtered tables for services\n",
    "\n",
    "service_sites_medical = service_sites[service_sites['ServiceOption_MedicalConditions_NA'] == 0]\n",
    "service_sites_mental = service_sites[service_sites['ServiceOption_MentalHealthConditions_NA'] == 0]\n",
    "service_sites_counseling = service_sites[service_sites['ServiceOption_CounselingTypesOffered_NA'] == 0]\n",
    "service_sites_therapy = service_sites[service_sites['ServiceOption_TherapyAndSupportiveApproaches_NA'] == 0]\n",
    "service_sites_aoda = service_sites[service_sites['ServiceOption_Aoda_NA'] == 0]\n",
    "\n",
    "list_of_service_sites = [service_sites_medical, \n",
    "                         service_sites_mental, \n",
    "                         service_sites_counseling, \n",
    "                         service_sites_therapy, \n",
    "                         service_sites_aoda]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 362,
   "metadata": {},
   "outputs": [],
   "source": [
    "# function that takes a table (zip codes and service sites) and a str for map_name  -> aggregates them and creates a map\n",
    "\n",
    "def make_sites_map(df, file_name):\n",
    "    df_site_count = df[['Site_Id', 'Zip']].groupby('Zip').agg('count').reset_index()\n",
    "    df_site_count.columns = ['Zip','Number of Sites']\n",
    "#     df_site_count = df_site_count.astype(int)\n",
    "#     a = './sites_by_service/'  + '_site_count.csv'\n",
    "    df_site_count.to_csv('./sites_by_service' + file_name + '.csv', index=False)\n",
    "    \n",
    "    # Creating the map centered at Wisconsin state\n",
    "    map = folium.Map(location=[44.808444, -89.673194], \n",
    "               tiles=\"cartodbpositron\", \n",
    "               zoom_start=6.8)\n",
    "\n",
    "    # Creating the Choropleth\n",
    "    folium.Choropleth(geo_data=json.loads(requests.get(wisconsin_geojson).text),\n",
    "             data=df_site_count,\n",
    "             columns=['Zip', 'Number of Sites'],\n",
    "             key_on='feature.properties.ZCTA5CE10', \n",
    "             fill_color='YlOrRd', fill_opacity=1, line_opacity=0.2, nan_fill_color=\"White\",\n",
    "             legend_name='Number of Sites').add_to(map)\n",
    "\n",
    "    \n",
    "    map.save(outfile = './sites_by_service/' + file_name + '.html')\n",
    "#     return map"
   ]
  },
