    "## Creating site density maps by service type"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 283,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Load data\n",
    "sites = pd.read_csv('../uwwi_datasets/uwwi_dataset_sites_encoded.csv')\n",
    "sites_zip = sites[['Site_Id', 'SiteAddressus_SiteAddressus_zip']]\n",
    "sites_zip = sites_zip.dropna()  # drop sites with no Site_Id or Zip\n",
    "sites_zip.columns = ['Site_Id','Zip']\n",
    "sites_zip['Zip'] = sites_zip['Zip'].astype(str).str[:5]\n",
    "\n",
    "services = pd.read_csv('../uwwi_datasets/uwwi_dataset_services_encoded.csv')\n",
    "services = services.dropna(subset = ['Service_Id']) # drop sites with no Service_Id"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 289,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Use ServiceOption_ cols to filter by service type\n",
    "# ServiceOption_<service>_NA = 1 implies that there is no service offered of <service> type\n",
    "# ServiceOption_<service>_NA = NA implies that there is at least one service offered of the <service> type\n",
    "# I change NA to 0 for ease\n",
    "\n",
    "service_filter_cols = [col for col in services.columns if 'NA' in col]\n",
    "services = services[['Service_Id','Site_Id'] + service_filter_cols]\n",
    "services = services.fillna(0)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 285,
   "metadata": {},
   "outputs": [],
   "source": [
    "service_sites = sites_zip.merge(services, on = 'Site_Id')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 290,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "Index(['Site_Id', 'Zip', 'Service_Id', 'ServiceOption_PaymentOptions_NA',\n",
       "       'ServiceOption_PopulationFocus_NA',\n",
       "       'ServiceOption_LanguagesOtherThanEnglishNew_NA',\n",
       "       'ServiceOption_Days_NA', 'ServiceOption_MentalHealthConditions_NA',\n",
       "       'ServiceOption_MedicalConditions_NA',\n",
       "       'ServiceOption_CounselingTypesOffered_NA', 'ServiceOption_AgeGroup_NA',\n",
       "       'ServiceOption_TherapyAndSupportiveApproaches_NA',\n",
       "       'ServiceOption_IssueOfFocus_NA', 'ServiceOption_Aoda_NA'],\n",
       "      dtype='object')"
